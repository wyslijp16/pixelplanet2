{
  "builds": [
    {
      "src": "dist/web.js",
      "use": "@vercel/node@3.0.12",
      "config": {
        "runtime": "nodejs18.x",
        "includeFiles": [
          "dist/**/*",
          "src/assets.json"
        ]
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "dist/web.js"
    }
  ]
}
