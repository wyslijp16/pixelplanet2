{
  "functions": {
    "src/web.js": {
      "runtime": "node tolerant",
      "includeFiles": [
        "src/assets.json"
      ]
    }
  },
  "builds": [
    {
      "src": "src/web.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/web.js"
    }
  ]
}
